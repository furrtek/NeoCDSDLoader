  <cheat desc="Infinite Time">
    <script state="on">
      <action>temp0 =maincpu.pb@10A83A</action>
    </script>
    <script state="run">
      <action condition="(frame % 120 == 0) AND (maincpu.pb@10A83A!=temp0 )">maincpu.pb@10A83A=60</action>
    </script>
  </cheat>

